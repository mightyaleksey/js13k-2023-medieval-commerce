{
  "name": "medieval-commerce",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "7.22.19",
    "@babel/eslint-parser": "7.22.15",
    "@rollup/plugin-babel": "6.0.3",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "clean-css": "5.3.2",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "ect-bin": "1.4.1",
    "esbuild-plugin-babel": "0.2.3",
    "eslint-plugin-flowtype": "8.0.3",
    "flow-bin": "0.216.1",
    "html-minifier": "4.0.0",
    "roadroller": "2.1.0",
    "standard": "17.1.0",
    "vite": "4.4.9"
  },
  "standard": {
    "globals": [
      "$Diff",
      "$Exact",
      "$Exports",
      "$KeyMirror",
      "$Keys",
      "$NonMaybeType",
      "$ReadOnly",
      "$ReadOnlyArray",
      "$Rest",
      "$TupleMap",
      "$Values",
      "Class",
      "Exclude",
      "Extract",
      "Omit",
      "OmitThisParameter",
      "Parameters",
      "Partial",
      "Pick",
      "Record",
      "Required",
      "ReturnType",
      "ThisParameterType"
    ],
    "parser": "@babel/eslint-parser",
    "plugins": [
      "flowtype"
    ]
  },
  "scripts": {
    "serve": "vite",
    "build": "cross-env LEVEL_2_BUILD=true vite build",
    "find-best-roadroller": "node find-best-roadroller.js",
    "build-with-best-roadroller": "cross-env USE_RR_CONFIG=true vite build",
    "lint": "standard",
    "vite": "vite serve"
  },
  "author": "Aleksey Litvinov"
}
